Name: <%= data.local.name %> <br />
Email: <%= data.local.email %> <br />
School: <%= data.local.school %> <br />
Joined: <%= data.createdAt %> <br />
<hr>
Achievements: <br>
<% data.achievements.forEach(function(el){ %>
  <%= el.year%> <br>
  <%= el.title%> <br>
  <%= el.description%> <br>
<% }) %>
<hr>
<% data.comments.forEach(function(comment){ %>
  <%= comment.content %><br />
  Comment Id: <%= comment._id %><hr />
<% }) %>
<form class="" action="/posts/<%= post._id %>/comments" method="post">
  <textarea name="content" rows="8" cols="40"></textarea><br>
  <input type="submit" name="submit" value="Submit">
</form>
<hr />
